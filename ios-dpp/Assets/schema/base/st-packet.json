{
  "$schema": "https://schema.dash.org/dpp-0-4-0/meta/dash-schema",
  "$id": "https://schema.dash.org/dpp-0-4-0/base/st-packet",
  "properties": {
    "objects": {
      "type": "array"
    },
    "contracts": {
      "type": "array"
    }
  },
  "oneOf": [
    {
      "properties": {
        "objects": {
          "maxItems": 0
        },
        "contracts": {
          "maxItems": 1
        }
      }
    },
    {
      "properties": {
        "objects": {
          "maxItems": 1000
        },
        "contracts": {
          "maxItems": 0
        }
      }
    }
  ],
  "required": [
    "objects",
    "contracts"
  ],
  "additionalProperties": false
}
